{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "SignalR_SignalRResourceForSpring_name": {
            "defaultValue": "ExcalifunSignalRGroup",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.SignalRService/SignalR",
            "apiVersion": "2025-01-01-preview",
            "name": "[parameters('SignalR_SignalRResourceForSpring_name')]",
            "location": "westeurope",
            "sku": {
                "name": "Free_F1",
                "tier": "Free",
                "size": "F1",
                "capacity": 1
            },
            "kind": "SignalR",
            "properties": {
                "enableMessageLogs": false,
                "serviceMode": "Default"
            }
        }
    ]
}